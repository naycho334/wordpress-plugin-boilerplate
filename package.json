{
  "name": "plugin-name",
  "version": "1.0.0",
  "description": "A boilerplate wordpress plugin",
  "main": "index.js",
  "variables": {
    "text_domain": "plugin-text-domain"
  },
  "scripts": {
    "generate-po": "php wp-cli.phar i18n make-pot ./ ./lang/en_US.po --domain=%npm_package_variables_text_domain%",
    "generate-mo": "bash ./generate-mo-files.sh",
    "watch": "npx mix watch",
    "build": "npx mix --production"
  },
  "author": "Samir El Khaouti",
  "author_email": "elkhaouti@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.2",
    "sass-loader": "^12.1.0",
    "zip-a-folder": "^1.1.5"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "lodash": "^4.17.21"
  }
}
